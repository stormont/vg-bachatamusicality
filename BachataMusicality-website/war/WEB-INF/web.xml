<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Info</servlet-name>
		<servlet-class>com.voyagegames.bachatamusicality.InfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Info</servlet-name>
		<url-pattern>/v1/info</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Log</servlet-name>
		<servlet-class>com.voyagegames.bachatamusicality.LogServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Log</servlet-name>
		<url-pattern>/v1/log</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogRetrieve</servlet-name>
		<servlet-class>com.voyagegames.bachatamusicality.LogRetrieveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogRetrieve</servlet-name>
		<url-pattern>/log/retrieve</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
